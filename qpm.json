      "versionRange": "^0.4.5",
      "additionalData": {}
    },
    {
      "id": "safestorage",
      "versionRange": "^1.1.1",
      "additionalData": {}
    },
    {
      "id": "libil2cpp",
      "versionRange": "^0.3.1",
