{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "ChatLib",
    "id": "chatlib",
    "version": "0.0.1",
    "url": "https://github.com/NuggoDEV/ChatLib",
    "additionalData": {
      "overrideSoName": "libchatlib.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
